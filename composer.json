{
  "name": "homer/yunba-push",
  "version": "1.0.0",
  "description": "Message pushing implementation with yunba as backend",
  "license": "proprietary",
  "type": "library",

  "autoload": {
    "files": [
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "Homer\\Push\\Yunba\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "spec\\Homer\\Push\\Yunba\\": "spec/"
    }
  },

  "config": {
    "preferred-install": "dist"
  },

  "minimum-stability": "dev",
  "prefer-stable": true,

  "require": {
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.1"
  },

  "require-dev": {
    "phpspec/phpspec": "^2.4"
  }
}
